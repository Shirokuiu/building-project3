.house__preview
  position absolute
  top 20px
  left 0
  z-index 20
  
  opacity 1
  
  transition opacity 0.3s ease
  &--red
    .house__preview-items
      &--green
        background-color #e6202f
        
        transition 0.2s ease
    .house__preview-number,
    .house__preview-text
      color #e7202f
      &--white
        color #ffffff
  &--hidden
    opacity 0
  
.house__popup
  center-absolute()
  
  z-index 30
  
  display flex
  justify-content center
  align-items center
  
  background-color rgba(000, 000, 000, 0.6);
  &--rooms6
    .house__popup-img
      background-image url('%=static=%content/popup-rooms6-img.png')
      
      transform rotateY(180deg)
  
.house__popup-content
  position relative
  
  box-sizing border-box
  width 445px
  height 445px
  padding 20px 30px
  
  background-color #ffffff
  
  border-radius 5px
  
.house__popup-exit
