@font-face {
  font-family: 'Sf-Ui-Display';
  font-style: normal;
  font-weight: 400;
  src: local('SFUIDisplay Regular'), local('SFUIDisplay-Regular'),
      url('../fonts/SFUIDisplay-Regular.woff2') format('woff2'), /* Super Modern Browsers */
      url('../fonts/SFUIDisplay-Regular.woff') format('woff') /* Modern Browsers */
}

@font-face {
  font-family: 'Sf-Ui-Display';
  font-style: normal;
  font-weight: 700;
  src: local('SFUIDisplay Bold'), local('SFUIDisplay-Bold'),
      url('../fonts/SFUIDisplay-Bold.woff2') format('woff2'), /* Super Modern Browsers */
      url('../fonts/SFUIDisplay-Bold.woff') format('woff') /* Modern Browsers */
}

@font-face {
  font-family: 'Proxima-Nova-Rg';
  font-style: normal;
  font-weight: 700;
  src: local('Proxima Nova Rg'), local('Proxima-Nova-Rg'),
      url('../fonts/ProximaNova-Bold.woff2') format('woff2'), /* Super Modern Browsers */
      url('../fonts/ProximaNova-Bold.woff') format('woff') /* Modern Browsers */
}

@font-face {
  font-family: 'Lato';
  font-style: normal;
  font-weight: 900;
  src: local('Lato Heavy'), local('Lato-Heavy'),
      url('../fonts/Lato-Heavy.woff2') format('woff2'), /* Super Modern Browsers */
      url('../fonts/Lato-Heavy.woff') format('woff') /* Modern Browsers */
}

body {
  font-size: 16px;
  line-height: 19px;
  font-weight 400
  font-family: 'Sf-Ui-Display', sans-serif;
  color: #000000;
  font-style: normal;
}

*::-webkit-input-placeholder {
  color #929292
  font-size: 16px;
  line-height: 19px;
  font-weight 300
  
  opacity: 1;
}

